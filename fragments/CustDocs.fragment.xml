							<Text wrapping="true" text="{Bill_To_Party}"/>
						</ui:template>
					</ui:Column>
					<ui:Column class="zc_contract_db_results_col" width="180px" autoResizable="true">
						<Text wrapping="true" text="Bill To Contact"/>
						<ui:template>
							<Text wrapping="true" text="{Bill_To_Contact}"/>
						</ui:template>
					</ui:Column>
					<ui:Column class="zc_contract_db_results_col" width="180px" autoResizable="true">
						<Text wrapping="true" text="Contract Admin"/>
						<ui:template>
							<Text wrapping="true" text="{Contract_Admin_Contact}"/>
						</ui:template>
					</ui:Column>
					<ui:Column class="zc_contract_db_results_col" width="180px" autoResizable="true">
						<Text wrapping="true" text="AOL"/>
						<ui:template>
							<Text wrapping="true" text="{Account_Ops_Lead}"/>
						</ui:template>
					</ui:Column>
					<ui:Column class="zc_contract_db_results_col" width="180px" autoResizable="true">
						<Text wrapping="true" text="Sales Rep"/>
						<ui:template>
							<Text wrapping="true" text="{Sales_Rep}"/>
						</ui:template>
					</ui:Column>
				</ui:columns>
			</ui:Table>
		</content>
		<beginButton>
			<Button text="Proceed" press="onPressProceed_Cust_Doc"/>
		</beginButton>
		<endButton>
			<Button text="Close" press="onClose_Cust_Doc"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>